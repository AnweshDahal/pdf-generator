{
  "NODE_ENV": {
    "remark": "Specifies the environment in which the app is running (development, production, etc.)",
    "example": "development"
  },
  "PORT": {
    "remark": "Port on which the service runs",
    "example": "3001"
  },
  "HOST": {
    "remark": "The Host on which the service runs",
    "example": "localhost"
  },
  "BASE_APP_API_URL": {
    "remark": "URL for base app api",
    "example": "http://localhost:3000"
  },
  "BASE_APP_URL": {
    "remark": "URL for base app application",
    "example": "http://localhost:5000"
  },
  "JWK_URL": {
    "remark": "URL for JWK",
    "example": "http://localhost:3000/.well-known/JWK.json"
  }
}
